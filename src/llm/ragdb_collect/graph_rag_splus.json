  "properties": ["suitability_score", "constraints"]
},
{
  "type": "CONTRAINDICATED_FOR",
  "source_type": "intervention",
  "target_type": "health_condition",
  "description": "특정 건강 상태에 금기",
  "properties": ["severity", "alternative"]
},
{
  "type": "IMPROVES",
  "source_type": "concept",
  "target_type": "metric",
  "description": "개선 효과",
  "properties": ["improvement_rate", "timeframe"]
}

